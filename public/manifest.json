{
  "name": "Nag Me",
  "description": "Nag me notify users of low priority routined tasks.",
  "version": "1.0.0",
  "manifest_version": 2,
  "homepage_url": "https://www.gauti.info/nag-me",
  "author": "Gauti Selvaraj",
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Nag Me",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
