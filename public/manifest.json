{
  "name": "Nag Me",
  "description": "Nag me notify users of low priority routined tasks.",
  "version": "1.1.1",
  "manifest_version": 2,
  "homepage_url": "https://www.gauti.info/nag-me",
  "author": "Gauti Selvaraj <me@gauti.info> (https://www.gauti.info/)",
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Nag Me",
    "default_popup": "index.html",
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
